#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	 int suma,i,N;
     cout << " ingresar el valor de N:";
	 cin >> N;
	 suma=0;
	 for ( i=1;i<=N;i=i+2)
		 suma=suma+i;
	 cout << "La suma de los "<< N <<"primeros numeros naturale es :"<<suma;
	 getch ();

}
